namespace pshenmic_dpp_v2_uniffi {};

[Enum]
interface IdentifierError {
  ParsingErrorBase58(string error);
};

interface IdentifierFFI {
    [Throws=IdentifierError]
    constructor(DynamicId id);

    string to_hex();
    string to_base58();
    string to_base64();
};

[Enum]
interface DynamicId {
  Str(string Str);
  Bytes(bytes Bytes);
  FFI(IdentifierFFI id);
};
